{
  "name": "whatsapp-manager-backend",
  "version": "1.0.0",
  "description": "Multi-instance WhatsApp Manager API",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "rm -rf /tmp/dist_build && tsc --outDir /tmp/dist_build && rm -rf /tmp/dist_build/core && cp -r /tmp/dist_build/* dist/",
    "start": "node dist/server.js",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.0.0",
    "@fastify/static": "^6.12.0",
    "@fastify/websocket": "^8.3.1",
    "@prisma/client": "^5.7.0",
    "@whiskeysockets/baileys": "^6.6.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.0",
    "ioredis": "^5.3.2",
    "mime-types": "^2.1.35",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.4",
    "@types/qrcode": "^1.5.6",
    "@types/uuid": "^9.0.7",
    "prisma": "^5.7.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
